    <extensionMap>
      <mapping ext="cfg" type="Ini" />
      <mapping pattern=".env" type="PLAIN_TEXT" />
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping pattern="LICENSE" type="PLAIN_TEXT" />
      <mapping ext="csv" type="PLAIN_TEXT" />
      <mapping ext="erb" type="PLAIN_TEXT" />
      <mapping ext="conf" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>